{
  "name": "swasthya-test-suite",
  "version": "1.0.0",
  "description": "Automated test, verify, and fix loop for Swasthya",
  "type": "module",
  "scripts": {
    "test:auto-fix": "node tests/test-runner.js",
    "test:api": "node tests/test-runner.js --suite api",
    "test:ui": "node tests/test-runner.js --suite ui",
    "test:integration": "node tests/test-runner.js --suite integration"
  },
  "dependencies": {
    "axios": "^1.6.2",
    "chai": "^4.3.10",
    "mocha": "^10.2.0",
    "supertest": "^6.3.3"
  }
}


